logger: {}
chartname: mychart
sharedValues:
  affinity: {}
  nodeSelector: {}
  podSecurityContext: {}
  resources: {}
  securityContext: {}
  tolerations: {}
  securityContext:
    runAsNonRoot: false
    runAsUser: 1000
globalConfig:
  metadata.labels:
  - strategy: newline
    key: mychart.labels
  metadata.name:
  - strategy: inline
    key: mychart.fullname
fileConfig:
  test/output/0600/mychart-generated/my-pod-pod.yaml:
    spec.containers[0].securityContext:
    - strategy: newline-yaml
      key: sharedValues.securityContext
