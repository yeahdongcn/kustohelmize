logger: {}
chartname: mychart
sharedValues:
  affinity: {}
  arg0:
    enabled: false
  http:
    enabled: true
  https:
    enabled: true
  webhook:
    enabled: false
  tls:
    enabled: false
  nodeSelector: {}
  podSecurityContext: {}
  ports:
  - containerPort: 6666
    name: some_other_port
    protocol: TCP
  resources: {}
  securityContext: {}
  tolerations: {}
globalConfig:
  metadata.labels:
  - strategy: newline
    key: mychart.labels
  metadata.name:
  - strategy: inline
    key: mychart.fullname
fileConfig:
  test/output/1000/mychart-generated/nginx-deployment-deployment.yaml:
    spec.template.spec.containers[0].args[0]:
    - strategy: control-if
      condition: sharedValues.arg0.enabled
    spec.template.spec.containers[0].ports[0]:
    - strategy: control-if
      condition: sharedValues.http.enabled
    spec.template.spec.containers[0].ports[2]:
    - strategy: control-if
      conditions:
      - key: sharedValues.webhook.enabled
      - key: sharedValues.tls.enabled
      conditionOperator: and
    spec.template.spec.containers[0].ports:
    - strategy: append-with
      key: sharedValues.ports
